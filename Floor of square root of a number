long long int floorSqrt(long long int x) 
    {
        if(x==0 || x==1)
            return x;
        long long int l=0, r=x;
        while(l<=r)
        {
            long long int mid=(l+r)/2;
            if(mid==x/mid)
                return mid;
            else if(mid>x/mid)
                r=mid-1;
            else
                l=mid+1;
        }
        return r; 
    }
    
Time Complexity  : O(log(n))
Space Complexity : O(1)
